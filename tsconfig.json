{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "ES2022",
    "outDir": "dist",
    "rootDir": "./보물창고",
    "moduleResolution": "node"
  },
  "include": ["보물창고/**/*.ts"],
  "exclude": ["node_modules"]
}
